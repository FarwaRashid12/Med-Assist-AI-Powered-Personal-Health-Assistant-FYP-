{
  "expo": {
    
    "name": "Med-Assist-AI-Powered-Personal-Health-Assistant-FYP-",
    "slug": "Med-Assist-AI-Powered-Personal-Health-Assistant-FYP-",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/icons/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Med-Assist to use the camera to scan prescriptions.",
        "NSUserNotificationUsageDescription": "Allow Med-Assist to send medicine reminders.",
        "NSContactsUsageDescription": "Allow Med-Assist to access your contacts to add emergency contacts."
      }
    },
   "android": {
  "permissions": ["CAMERA", "NOTIFICATIONS", "VIBRATE", "READ_CONTACTS"],
  "package": "com.medassist.fyp",
  "edgeToEdgeEnabled": true,
  "adaptiveIcon": {
    "foregroundImage": "./src/assets/icons/adaptive-icon.png",
    "backgroundColor": "#ffffff"
  }
}
,
    "plugins": [
      [
      "expo-image-picker",
        {
          "cameraPermission": "Allow Med-Assist to access your camera."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#0077B6"
        }
      ]
    ],
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "dedacea6-b68f-421d-9849-6f2316e3acde",
        "GOOGLE_VISION_API_KEY": "AIzaSyAP98rAgIJJC8vsdwtgdNE2Zdf6WMxFcWs"
      }
    }
  }
}
